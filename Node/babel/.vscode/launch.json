{

    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "name": "current file",
        "program": "${file}",
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
        "sourceMaps": true,
        "env": {
            "BABEL_ENV": "debug"
        }
    }]
}